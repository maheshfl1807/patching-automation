<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

    <PropertyGroup>
        <!-- use C# 9 -->
        <LangVersion>9.0</LangVersion>
    </PropertyGroup>

    <PropertyGroup>
        <NoWarn>SA1633, SA1308, SA1309, SA1311, SA1101</NoWarn>
        <!--
        SA1633 - File header.
        SA1308 - Don't start private field with 's_'.
        SA1309 - Don't start private field with '_'.
        SA1311 - Static readonly fields should begin with uppercase letter.
        SA1101 - Prefix local calls with "this".
        -->
    </PropertyGroup>

</Project>